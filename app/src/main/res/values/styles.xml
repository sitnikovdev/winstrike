<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppBase" parent="Theme.AppCompat.Light.NoActionBar" />

    <style name="AppTheme" parent="AppBase">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.TranslucentNavigation" parent="AppTheme">
        <item name="android:windowTranslucentNavigation" tools:targetApi="kitkat">true</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>

    <style name="MainToolBarTheme" parent="@style/AppBase">
        <item name="colorControlHighlight">@color/color_ripple</item>
    </style>

    <!--Views-->
    <style name="TabLayoutProfile" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">17sp</item>
        <item name="android:fontFamily">@font/stem_regular</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="pushSwitch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingLeft">48dp</item>
        <item name="android:paddingRight">48dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:fontFamily">@font/stem_regular</item>
        <item name="android:textColor">@color/color_primary_dark</item>
    </style>


